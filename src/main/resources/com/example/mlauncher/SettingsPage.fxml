<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #E0E0E0;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mlauncher.controller.SettingsPageController">
   <children>
      <VBox prefHeight="400.0" prefWidth="200.0" spacing="6.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <CheckBox contentDisplay="CENTER" mnemonicParsing="false" prefHeight="50.0" prefWidth="219.0" text="BE builds">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </CheckBox>
            <CheckBox contentDisplay="CENTER" mnemonicParsing="false" prefHeight="50.0" prefWidth="219.0" text="old BE builds">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </CheckBox>
            <CheckBox contentDisplay="CENTER" mnemonicParsing="false" prefHeight="50.0" prefWidth="219.0" text="old versions">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </CheckBox>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="198.0" text="Versions folder" textAlignment="CENTER" />
            <HBox prefWidth="200.0">
               <children>
                  <TextField prefHeight="25.0" />
                  <Button mnemonicParsing="false" prefHeight="25.0" style="-fx-background-color: #9ACD32;" text="Open" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="6.0" />
         </padding>
      </VBox>
      <Button mnemonicParsing="false" style="-fx-background-color: #9ACD32;" text="Reset settings" AnchorPane.bottomAnchor="6.0" AnchorPane.leftAnchor="6.0" />
   </children>
</AnchorPane>
